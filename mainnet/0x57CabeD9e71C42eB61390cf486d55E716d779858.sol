// File contracts/oracle/WrappedUniswapV3PriceSource.sol


pragma solidity ^0.8.0;





library SafeCast {
    function toUint128(uint256 y) internal pure returns (uint128 z) {
        require((z = uint128(y)) == y);
    }
}

contract WrappedUniswapV3PriceSource is OwnableUpgradeable, PriceSource {
   
   string public constant name = "UniswapV3";

   using SafeMathUpgradeable for uint;

   struct TokenConfig {
        address underlying;
        bool fixedUsd; //if true,will return 1*e36/baseUnit
        uint32 twapPeriod;
        address pool;
    }

    mapping(address => TokenConfig) public tokenConfigs;

    address public uniswapV3Factory;
    address public usdc;

    function initialize(
        address _uniswapV3Factory,
        address _usdc
    ) initializer public {
        uniswapV3Factory = _uniswapV3Factory;
        usdc = _usdc;

        OwnableUpgradeable.__Ownable_init();
    }

   function _setTokenConfig(address underlying, address pool, uint32 twapPeriod, bool fixedUsd) public onlyOwner{

        TokenConfig storage tokenConfig = tokenConfigs[underlying];
        tokenConfig.underlying = underlying;
        tokenConfig.pool = pool;
        tokenConfig.twapPeriod = twapPeriod;
        tokenConfig.fixedUsd = fixedUsd;

    }

    function _setTokenConfigs(TokenConfig[] memory configs) public onlyOwner{

        for(uint i = 0; i < configs.length; i++){
            TokenConfig memory tokenConfig = configs[i];
            _setTokenConfig(tokenConfig.underlying, tokenConfig.pool, tokenConfig.twapPeriod, tokenConfig.fixedUsd);
        }

    }

    /**
     * @notice Get the price of an token asset.
     * @param token The token asset to get the price of.
     * @return The token asset price in USD as a mantissa (scaled by 1e8).
    */
    function getPrice(address token) external override view returns (uint){

        TokenConfig memory tokenConfig = tokenConfigs[token];
        if(tokenConfig.fixedUsd) {//if true,will return 1*e36/baseUnit
            return 1e8;
        }

        uint amountIn;
        if(token == 0xEeeeeEeeeEeEeeEeEeEeeEEEeeeeEeeeeeeeEEeE){
            amountIn = 1e18;
        }else{
            uint decimals = IERC20(token).decimals();
            amountIn = 10 ** decimals;
        }

        uint256 amountOut = _fetchAmountFromSinglePool(tokenConfig.underlying,amountIn, usdc, tokenConfig.pool, tokenConfig.twapPeriod);

        return uint(amountOut).mul(100);
   }

    
    function fetchCurrentTicks(address _pool, uint32 _twapPeriod) public view returns (int24 spotTick, int24 twapTick) {
        (spotTick,) = OracleLibrary.getBlockStartingTickAndLiquidity(_pool);
        (twapTick, ) = OracleLibrary.consult(_pool, _twapPeriod);
    }
   
    function _fetchAmountFromSinglePool(
        address _tokenIn,
        uint256 _amountIn,
        address _tokenOut,
        address _pool,
        uint32 _twapPeriod
    ) internal view returns (uint256 amountOut) {
        // Leave ticks as int256s to avoid solidity casting
        (int256 spotTick , int256 twapTick) =fetchCurrentTicks(_pool,_twapPeriod);

        // Return min amount between spot price and twap
        // Ticks are based on the ratio between token0:token1 so if the input token is token1 then
        // we need to treat the tick as an inverse
        int256 minTick;
        if (_tokenIn < _tokenOut) {
            minTick = spotTick < twapTick ? spotTick : twapTick;
        } else {
            minTick = spotTick > twapTick ? spotTick : twapTick;
        }

        return
            OracleLibrary.getQuoteAtTick(
                int24(minTick), // can assume safe being result from consult()
                SafeCast.toUint128(_amountIn),
                _tokenIn,
                _tokenOut
            );
    }





}